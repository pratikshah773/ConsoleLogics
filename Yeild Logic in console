using System;
using System.Collections;

namespace yield
{
    class Program
    {
        static void Main(string[] args)
        {
            foreach (var item in func(2,10))
            {
                Console.WriteLine(item);
            }
        }

        public static IEnumerable func(int start, int number)
        {
            for (int i   = 0; i < number; i++)
            {
                int b= start + 2 * i;
                yield return b;

            }
        }
    
    
    }

   
}
